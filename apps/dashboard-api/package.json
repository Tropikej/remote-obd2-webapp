{
  "name": "@dashboard/api",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "npm run build --workspace @dashboard/shared && tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "rotate-dongle-keys": "ts-node -r tsconfig-paths/register src/jobs/rotate-dongle-keys.ts",
    "test:agent-heartbeat": "ts-node -r tsconfig-paths/register src/scripts/test-agent-heartbeat.ts",
    "test:crypto": "ts-node -r tsconfig-paths/register src/scripts/validate-dongle-token-crypto.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@dashboard/shared": "0.0.0",
    "@prisma/client": "^5.22.0",
    "argon2": "^0.44.0",
    "connect-pg-simple": "^9.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "ioredis": "^5.8.2",
    "ioredis-mock": "^8.13.1",
    "pg": "^8.11.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/pg": "^8.11.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
