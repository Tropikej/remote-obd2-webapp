{
  "name": "@dashboard/shared",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./api/*": {
      "types": "./dist/api/*.d.ts",
      "require": "./dist/api/*.js"
    },
    "./protocols/*": {
      "types": "./dist/protocols/*.d.ts",
      "require": "./dist/protocols/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
